[Unit]
After=dappsteros-message-bus.service
After=docker.service
Description=CasaOS App Management Service

[Service]
ExecStartPre=/usr/bin/dappsteros-app-management -v
ExecStart=/usr/bin/dappsteros-app-management -c /etc/dappsteros/app-management.conf
PIDFile=/var/run/dappsteros/app-management.pid
Restart=always
Type=notify

[Install]
WantedBy=multi-user.target
